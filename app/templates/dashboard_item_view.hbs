<h2 class="col-sm-12"><a href="{{invoke model method="appURL"}}">{{model.attributes.name}}</a></h2>
<div class="col-sm-12 clearfix">
  <data><span class="glyphicons play-button"></span><b>{{model.attributes.runs}}</b> Runs</data>
  <ul>
    {{#each model.attributes.tags}}
    <li class="tag">
      <a href="/{{this.name}}">{{this.name}}</a>
    {{/each}}
    {{#unless model.attributes.tags.length}}
    <li class="tag no-tags">
      <a><span class="glyphicons lock"></span> NO TAGS (PRIVATE)</a>
    {{/unless}}
  </ul>
  <time>{{dateAgo model.attributes.created}}</time>
</div>
{{#if editmode}}
<button class="delete" type="button">
  <span>Delete</span>
  <span class="glyphicons circle-remove"></span>
</button>
{{/if}}