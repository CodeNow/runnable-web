<h3>
  <a href="{{invoke model method='appURL'}}">{{model.attributes.name}}</a>
</h3>
<p class="runnable-stats">
  {{#with model.attributes}}
  {{#if ../isimage}}
  {{view "runnable_stats" runs=runs views=views copies=copies}}
  &bull;
  <span>Published {{dateAgo created}}</span>&bull;
  {{else}}
  <span>Created {{dateAgo created}}</span>&bull;
  {{/if}}
  <span class="tags">
    {{#each tags}}
    {{view "tag_link" name=name}}
    {{/each}}
  </span>
  {{#if ../editmode}}
  <button class="delete" type="button">x</button>
  {{/if}}
  {{/with}}
</p>