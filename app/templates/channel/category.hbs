{{view "header_view" model=user context=.}}
<div class="content">
  <div class="container">
    <h1>Frameworks</h1>
    <div class="span3">
      <h3>Browse code by category</h3>
      <ul>
        {{#each categories.models}}
        {{#with attributes}}
        <li>
          <a href="">{{name}}</a>
        </li>
        {{/with}}
        {{/each}}
        <li><a href="">Browse all code</a>
      </ul>
    </div>
    <div class="span9 last">
      <ul>
        {{#each channels.models}}
        {{#with attributes}}
        <li>
          <span>{{name}}</span>
          <span>{{description}}</span>
        </li>
        {{/with}}
        {{/each}}
        <li>Your code here</li>
      </ul>
    </div>
  </div>
</div>
{{view "footer_view" model=user context=.}}