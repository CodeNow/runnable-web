<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
      <h4 class="modal-title">Select a Services</h4>
    </div>
    <div class="modal-body">
      <form class="step1 form-horizontal" role="form">
        <div class="form-group">
          <div class="col-sm-12">
            <ul>
              {{#each collection.models}}
              <li>
                <a href="javascript:" data-id="{{id}}">
                  <img src="/images/provider-icons/icon-codeigniter.png" height="24" width="22">
                  <span>{{attributes.name}}</span>
                </a>
              </li>
              {{/each}}
            </ul>
            <a>Create New Service</a>
          </div>
          <div class="col-sm-12">
            <a>Select Existing Service</a>
            <input class="form-control" placeholder="Service Name" autofocus>
            <button type="button" class="orange">Next</button>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>