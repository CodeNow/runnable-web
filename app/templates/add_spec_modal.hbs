<div class="modal-dialog">
  <div class="modal-content">
    <div class="modal-header">
      <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
      <h4 class="modal-title">Select an API</h4>
    </div>
    <div class="modal-body">
      <form class="step1 form-horizontal" role="form">
        <div class="form-group">
          <div class="col-sm-12">
            <ul>
              {{#each collection.models}}
              <li>
                <a class="existing" href="javascript:" data-id="{{id}}">
                  <img src="/images/provider-icons/icon-codeigniter.png" height="24" width="22">
                  <span>{{attributes.name}}</span>
                </a>
              </li>
              {{/each}}
            </ul>
            <a class="new" href="javascript:">Add New API</a>
          </div>
        </div>
      </form>
    </div>
  </div>
</div>