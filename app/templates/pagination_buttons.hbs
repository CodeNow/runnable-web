<div class="row">
  <div class="col-md-10 col-sm-9 col-md-offset-2 col-sm-offset-3">
    <ul class="pagination">

      {{#if showPrevLink}}
        <li class="prev">
          <a href="{{prevQueryString}}">
            <span class="glyphicons chevron-left"></span>Previous
          </a>
          <li><a href="{{indexQueryString}}">1</a>
        {{#if showLeftElipsis}}
          <li class="ellipsis">• • •
        {{/if}}
      {{/if}}

      <li class="active">{{humanCurrentPage}}

      {{#if showNextLink}}
        {{#if showRightElipsis}}
          <li class="ellipsis">• • •
        {{/if}}
        <li>
          <a href="{{lastQueryString}}">{{humanLastPage}}</a>
        <li class="next">
          <a href="{{nextQueryString}}">
            Next <span class="glyphicons chevron-right"></span>
          </a>
      {{/if}}

    </ul>
  </div>
</div>
