{{view 'git_card'}}

{{#each collection.models}}
{{#with attributes}}
  <div class="col-sm-4">
    {{#if attributes.url}}
    <a href="{{url}}" data-name="{{name}}">
    {{else}}
    <a href="/new/{{name}}" data-name="{{name}}">
    {{/if}}
      {{view 'channel_image' name=name}}
      <div class="description">
        <h2>{{name}}</h2>
        {{#if description}}
        <p>{{description}}
        {{/if}}
      </div>
    </a>
  </div>
{{/with}}
{{/each}}
