{{#each collection.models}}
  <li>
    <h2 class="col-sm-12"><a href="{{attributes.appUrl}}">{{attributes.name}}</a></h2>
    <div class="col-sm-12 clearfix">
      <data><span class="glyphicons play-button"></span><b>{{attributes.runs}}</b> Runs</data>
      <ul>
        {{#each attributes.tags}}
        <li class="tag">
          <a>{{this.name}}</a>
        {{/each}}
      </ul>
      <time>{{dateAgo attributes.created}}</time>
    </div>
    <a class="user-img">
      {{view 'user_image' model=owner}}
    </a>
{{/each}}
