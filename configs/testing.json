{
  "cookieKey": "runnable.sid",
  "cookieSecret": "$up3r,$3<r3t",
  "concurrentFileStreams": 25,
  "disableWebsockets": false,
  "expires": "2 minutes",
  "usebuild": true,
  "projectIdleTimeExpires": 1,
  "abortOnError": false,
  "gears": "{HOME_DIR}/.runnable/gears.runnablecloud.com.json",
  "seoCacheTimeout": "10 seconds",
  "mongo": "mongodb://127.0.0.1:27017/runnable_{RAND_NUM}",
  "openshiftDomain": "runnablecloud.com",
  "sshReconnectInterval": "30 seconds",
  "sshPingInterval": "30 seconds",
  "git": {
    "name": "Runnable",
    "email": "git@runnable.com"
  },
  "openshift_ssh": {
    "privateKey": "{HOME_DIR}/.runnable/openshift.rsa",
    "publicKey": "{HOME_DIR}/.runnable/openshift.rsa.pub"
  },
  "logging": {
    "output_to_console": false
  },
  "port" : 3000,
  "defaultRunnables": {
    "node.js": {
      "hello": {
        "name": "My Runnable",
        "src": "{CURR_DIR}/runnables/node.js/hello",
        "defaultFile": "server.js"
      },
      "twitter": {
        "name": "Get friends with Twitter",
        "src": "{CURR_DIR}/runnables/node.js/twitter",
        "defaultFile": "server.js"
      }
    },
    "python": {
      "hello": {
        "name": "My Runnable",
        "src": "{CURR_DIR}/runnables/python/hello",
        "defaultFile": "app.py"
      }
    },
    "php": {
      "hello": {
        "name": "My Runnable",
        "src": "{CURR_DIR}/runnables/php/hello",
        "defaultFile": "php/index.php"
      }
    }
  },
  "zuulDomains": {
    "facebook": "facebook.runnablecloud.com",
    "twitter": "twitter.runnablecloud.com",
    "google": "google.runnablecloud.com"
  },
  "zuulRootUrls": {
    "facebook": "https://facebook.runnablecloud.com",
    "twitter": "https://twitter.runnablecloud.com",
    "google": "https://google.runnablecloud.com"
  },
  "googleBrowserApiKey": "AIzaSyDHEDrHoZseIZExxkKAFQ0GIhdrONMUsac",
  "repoCache": "{RAND_DIR}/projects",
  "serviceCheckInterval": "1 second",
  "serviceTimeout": "3 second",
  "www_if": "127.0.0.1",
  "rateLimit" : {
    "requestsPerInterval" : 1000,
    "interval"            : 1000,
    "blockTime"           : 30000
  }
}
